@model EVA3AJAX.Models.UnidadHerramienta
@{
    ViewData["Title"] = "Detalles";
}
<h1>Detalles de la Unidad de Herramienta</h1>
<div class="card mb-4">
    <div class="card-body">
        <h4>Unidad de Herramienta</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">Número de Serie</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.NumeroSerie)</dd>
            <dt class="col-sm-2">Estado</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.Estado)</dd>
            <dt class="col-sm-2">ID de Herramienta</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.HerramientaId)</dd>
        </dl>
    </div>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id" class="btn btn-warning">Editar</a> |
    <a asp-action="Index" class="btn btn-secondary">Volver a la Lista</a>
</div>


