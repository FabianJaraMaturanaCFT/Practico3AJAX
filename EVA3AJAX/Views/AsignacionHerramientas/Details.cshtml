@model EVA3AJAX.Models.AsignacionHerramienta
@{
    ViewData["Title"] = "Detalles";
}
<h1>Detalles de la Asignación de Herramienta</h1>
<div>
    <h4>Asignación de Herramienta</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Unidad de Herramienta
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UnidadHerramientaId)
        </dd>
        <dt class="col-sm-2">
            Usuario
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UsuarioId)
        </dd>
        <dt class="col-sm-2">
            Fecha de Asignación
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaAsignacion)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id" class="btn btn-warning">Editar</a> |
    <a asp-action="Index" class="btn btn-secondary">Volver a la Lista</a>
</div>
